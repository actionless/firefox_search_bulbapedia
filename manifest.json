{
	"manifest_version": 3,
	"name": "search_bulbapedia",
	"version": "0.4",
	"description": "Search Bulbapedia",

	"icons": {
		"48": "icons/icon.png",
		"96": "icons/icon@2x.png"
	},

	"chrome_settings_overrides": {
	  "search_provider": {
		"name": "Bulbapedia",
		"search_url": "https://bulbapedia.bulbagarden.net/w/index.php?title=Special:Search&search={searchTerms}",
		"suggest_url": "https://bulbapedia.bulbagarden.net/w/api.php?action=opensearch&search={searchTerms}",
		"keyword": "b",
		"favicon_url": "/icons/favicon.ico"
	  }
	}

	"browser_specific_settings": {
		"gecko": {
			"id": "{0a4643c4-de3a-4164-afd6-f5134f42b8b6}",
			"strict_min_version": "106.0"
		}
	}
}
